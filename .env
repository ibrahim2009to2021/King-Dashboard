# AI Marketing Engine - Environment Configuration
# Copy this file to .env and update with your actual values

# Application Settings
REACT_APP_APP_NAME=AI Marketing Engine
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_URL=https://api.aimarketingengine.com
REACT_APP_API_TIMEOUT=10000

# Benchmark Intelligence API
REACT_APP_BENCHMARK_API_KEY=your_benchmark_api_key_here
REACT_APP_BENCHMARK_API_URL=https://api.benchmarks.aimarketingengine.com

# WhatsApp Business Integration
REACT_APP_WHATSAPP_API_KEY=your_whatsapp_business_api_key
REACT_APP_WHATSAPP_BUSINESS_NUMBER=your_business_phone_number

# Analytics and Tracking
REACT_APP_GOOGLE_ANALYTICS_ID=your_google_analytics_id
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token

# Voice Recognition Settings
REACT_APP_VOICE_LANGUAGE=en-US
REACT_APP_VOICE_TIMEOUT=30000

# Feature Flags
REACT_APP_ENABLE_VOICE_COMMANDS=true
REACT_APP_ENABLE_WHATSAPP_SHARING=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true

# Security Settings
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_CSRF_PROTECTION=true

# Cache Settings
REACT_APP_CACHE_DURATION=300000
REACT_APP_OFFLINE_CACHE_SIZE=50MB

# Deployment Settings
REACT_APP_BASE_URL=https://app.aimarketingengine.com
REACT_APP_CDN_URL=https://cdn.aimarketingengine.com