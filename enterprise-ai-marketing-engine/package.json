    "react-window": "^1.8.10",
    "@types/react-window": "^1.8.8",
    "core-js": "^3.35.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/node": "^20.10.7",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "@vitest/coverage-v8": "^1.1.3",
