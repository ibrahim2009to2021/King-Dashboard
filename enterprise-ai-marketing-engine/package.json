    "@types/papaparse": "^5.3.14",
    "xlsx": "^0.18.5",
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "clsx": "^2.1.0",
    "react-dropzone": "^14.2.3",
    "react-beautiful-dnd": "^13.1.1",
    "@types/react-beautiful-dnd": "^13.1.8",
