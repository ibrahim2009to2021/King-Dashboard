# AI Marketing Engine - Environment Configuration
# Copy this file to .env and update with your actual values

# Application Settings
VITE_APP_NAME=AI Marketing Engine
VITE_VERSION=1.0.0
VITE_ENVIRONMENT=development

# API Configuration
VITE_API_URL=https://api.aimarketingengine.com
VITE_API_TIMEOUT=10000

# Benchmark Intelligence API
VITE_BENCHMARK_API_KEY=your_benchmark_api_key_here
VITE_BENCHMARK_API_URL=https://api.benchmarks.aimarketingengine.com

# WhatsApp Business Integration
VITE_WHATSAPP_API_KEY=your_whatsapp_business_api_key
VITE_WHATSAPP_BUSINESS_NUMBER=your_business_phone_number

# Analytics and Tracking
VITE_GOOGLE_ANALYTICS_ID=your_google_analytics_id
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Voice Recognition Settings
VITE_VOICE_LANGUAGE=en-US
VITE_VOICE_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_VOICE_COMMANDS=true
VITE_ENABLE_WHATSAPP_SHARING=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true

# Security Settings
VITE_SESSION_TIMEOUT=3600000
VITE_CSRF_PROTECTION=true

# Cache Settings
VITE_CACHE_DURATION=300000
VITE_OFFLINE_CACHE_SIZE=50MB

# Deployment Settings
VITE_BASE_URL=https://app.aimarketingengine.com
VITE_CDN_URL=https://cdn.aimarketingengine.com